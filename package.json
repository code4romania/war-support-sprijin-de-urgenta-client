    "cypress:headless": "cypress run",
    "e2e": "env-cmd -f ./env/.env.development next build && env-cmd -f ./env/.env.development start-server-and-test start http://localhost:3000 cypress",
    "e2e:headless": "env-cmd -f ./env/.env.development start-server-and-test start http://localhost:3000 cypress:headless",
